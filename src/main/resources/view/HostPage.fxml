<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.BoxBlur?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.HelloController">
   <children>
      <TextField fx:id="nameField" layoutX="263.0" layoutY="97.0">
         <effect>
            <BoxBlur height="0.3" iterations="0" width="2.0">
               <input>
                  <DropShadow />
               </input>
            </BoxBlur>
         </effect></TextField>
      <Label fx:id="nameLabel" layoutX="132.0" layoutY="100.0" text="Enter your name">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Label>
      <Label fx:id="portLabel" layoutX="131.0" layoutY="175.0" text="Enter server Port">
         <font>
            <Font name="System Bold" size="14.0" />
         </font></Label>
      <Button layoutX="103.0" layoutY="272.0" mnemonicParsing="false" onAction="#setNameLabel" text="Save Fields" />
      <Button layoutX="381.0" layoutY="272.0" mnemonicParsing="false" onAction="#StartBoard" prefHeight="25.0" prefWidth="62.0" text="Start" />
      <TextField fx:id="portField" layoutX="263.0" layoutY="172.0">
         <effect>
            <BoxBlur height="0.3" iterations="0" width="2.0">
               <input>
                  <DropShadow />
               </input>
            </BoxBlur>
         </effect>
      </TextField>
   </children>
</AnchorPane>
